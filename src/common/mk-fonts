#!/usr/bin/perl -w

# 
#       Simple mono-spaced ASCII fonts
# 
#         Copyright (c) 2002-2003 Jim Peters <http://uazu.net/>.
#         Released under the GNU GPL version 2 as published by the
#         Free Software Foundation.  See the file COPYING for details,
#         or visit <http://www.gnu.org/copyleft/gpl.html>.
#

$DEBUG= 0;

die unless open OUT, ">fonts.c";

print OUT <<END;
//
//      Simple mono-spaced ASCII fonts
//
//        Copyright (c) 2002-2003 Jim Peters <http://uazu.net/>.
//        Released under the GNU GPL version 2 as published by the
//        Free Software Foundation.  See the file COPYING for details,
//        or visit <http://www.gnu.org/copyleft/gpl.html>.
//
//	This file was generated using the mk-fonts script.  Make 
//	adjustments there.
//
//      I designed the 8x16 and 10x20 fonts for my own use and they are
//      both loosely based around the first text-mode font I created
//      for my original Linux box, which was a 9x14 font.  This was very
//      comfortable on the eyes, however it required non-square pixels
//      to look right.  These were very easy to generate using a custom
//      SVGATextMode config, but due to the advancement of technology,
//      that font is no longer usable.  These fonts were designed to be 
//	programmer-friendly -- for example, the parentheses () are nice 
//	and obvious.
//
//      The 6x8 and 6x12 fonts were knocked together to provide
//      readable smaller monospace text fonts.
//

END

sub do_font {
    my ($ww, $hh, $dat)= @_;
    my @dat= split(/\n/, $dat);
    my $n_lin= (0+@dat) / $hh;
    my $n_col= int((length($dat[0]) + 1) / $ww);
    my $n_ch= $n_col * $n_lin;
    my $brk= ($hh == 20) ? 10 : 999;

    printf OUT "short font%dx%d[]= {\n", $ww, $hh;
    printf OUT "   %d, %d, 32, %d,\n", $ww, $hh, $n_ch;

    for my $ch (0..$n_ch-1) {
	print "Character $ch\n" if $DEBUG;
	print OUT "   ";
	for my $ln (0..$hh-1) {
	    my $dd= substr($dat[$ln + $hh * int($ch/$n_col)], ($ch%$n_col)*$ww, $ww);
	    print "$dd\n" if $DEBUG;
	    if ($ww != ($dd =~ tr/-= 0/0001/)) {
		die "Bad data for character $ch: $dd";
	    }
	    my $val= 1;
	    while ($dd ne '') {
		$dd =~ s/([01])$//;
		$val= $val*2 + $1;
	    }
	    print OUT "$val,";
	    print OUT "\n   " if ($ln + 1 < $hh && ($ln + 1)%$brk == 0);
	}
	print OUT "    // '" . chr(32 + $ch) . "'\n";
    }

    print OUT "};\n\n";
}

do_font 6, 8, <<END;
